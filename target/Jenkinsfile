pipeline {
    agent any    
    stages {
        stage('SonarQube Code Qaulity Check') {
            agent {
                docker {
                    image 'maven'
                }
            }
            steps {
                script {
                    sh 'chmod -R 777 /.m2/repository'
                    withSonarQubeEnv(credentialsId: 'sonar-token') { 
                     sh 'mvn --version'
                     sh 'mvn clean package sonar:sonar'
                 }

                }
            }
        }
    }
}